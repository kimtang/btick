{
  "global": {
    "data": "data",
    "audit": "audit",
    "instance": 1,
    "host": ["localhost"],
    "passwd":"btpasswd",
    "daemon":false,
    "solacePubSub":{
      "SESSION_HOST":"host.docker.internal",
      "SESSION_VPN_NAME":"default",
      "SESSION_USERNAME":"admin",
      "SESSION_PASSWORD":"admin",
      "endpoint":"%plantName%/%deploymentEnvironment%/%subsys%",
      "topic":"%plantName%/%deploymentEnvironment%/%subsys%"
    }
  },

  "admin": {
    "process": {
      "bus": {
        "library": "randomSeed,pm,setPort,hopen,bus.server,heartbeat.client,berror.client,pm,daemon",
        "arg": {
          "setPort": "%basePort% + 2"
        }
      },
      "tick": {
        "library": "randomSeed,setPort,pm,hopen,bus.client,tick.stream,tick.hft,solacePubSub,solacePubSub.sub,heartbeat.solace.sub,monitor.cep.server,usage.server,berror.server,info.server",
        "arg": {
          "tick": "hft",
          "setPort": "%basePort% + 3"
        }
      },
      "replay": {
        "library": "randomSeed,setPort,pm,hopen,bus.client,tick.replay,heartbeat.client,berror.client",
        "arg": {
          "setPort": "%basePort% + 4"
        }
      },
      "rdb": {
        "library": "randomSeed,setPort,pm,hopen,bus.client,tick.sub,heartbeat.client,berror.client",
        "arg": {
          "setPort": "%basePort% + 5"
        }
      },
      "cdb": {
        "library": "randomSeed,setPort,pm,hopen,bus.client,tick.cdb,heartbeat.client,berror.client",
        "arg": {
          "setPort": "%basePort% + 6",
          "daemon": true
        }
      },
      "ctp":{
        "library": "randomSeed,setPort,pm,hopen,bus.client,tick.sub,tick.cep,tick.ctp,websocket,heartbeat.client,berror.client,info.client",
        "arg": {
          "setPort": "%basePort% + 7"
        }        
      },
      "monitorCep":{
        "library": "randomSeed,setPort,pm,hopen,bus.client,tick.sub,tick.cep,monitor.cep.client,heartbeat.client,berror.client,info.client",
        "arg": {
          "setPort": "%basePort% + 8"
        }        
      },
      "solacePubSub":{
        "library": "randomSeed,setPort,pm,hopen,bus.client,solacePubSub,heartbeat.solace.pub",
        "arg": {
          "setPort": "%basePort% + 10"
        }                  
      }       
    }
  },

  "backfill": {
    "process": {

      "bus": {
        "library": "randomSeed,setPort,pm,hopen,bus.server,heartbeat.client,berror.client,pm,daemon",
        "arg": {
          "setPort": "%basePort% + 1"
        }
      },

      "rdb": {
        "library": "randomSeed,setPort,pm,hopen,bus.client,heartbeat.client,backfill.rdb,berror.client",
        "arg": {
          "setPort": "%basePort% + 2"
        }
      },

      "tick": {
        "library": "randomSeed,setPort,pm,hopen,bus.client,heartbeat.client,backfill.tick,berror.client",
        "arg": {
          "setPort": "%basePort% + 3"
        }
      },

      "replay": {
        "library": "randomSeed,setPort,pm,hopen,bus.client,heartbeat.client,backfill.replay,berror.client",
        "arg": {
          "setPort": "%basePort% + 4"
        }
      }

    }
  }

}